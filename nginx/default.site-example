# Default server configuration
#
server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name _;

	location / {
		proxy_pass http://127.0.0.1:8000;
	}

	location /downloader {
        	proxy_pass http://127.0.0.1:1234;
	}

	location /feed {
        	proxy_pass http://127.0.0.1:1234;
	}
}
